  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.1",
    "sass": "^1.32.12"
  }
}
